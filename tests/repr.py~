#!/usr/bin/python3


class Person:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __repr__(self):
        rep = 'person is' + self.name + 'his age is' + str(self.age)
        return rep
    def __str__(self):
        return f'{self.name}, {self.age}'

me = Person("Andrew", 30)
print(me.__repr__())
print(me.__str__())
